title = {{Vortex methods and their application to trailing wake vortex simulations}},
author = {Winckelmans, Gr\'{e}goire and Cocle, Roger and Dufresne, Louis and Capart, Rapha\"{e}l},
doi = {10.1016/j.crhy.2005.05.001},
